<PropertyList>

 <path>Models/dme-bi-f8e.ac</path>
 <texture-path>Models/Textures</texture-path>


 	<model>
		<name>Bipolaire</name>
		<path>Aircraft/F-8E-Crusader/Instruments/Bipolaire-f8e.xml</path>
		<offsets>
			<x-m>0.002</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.0</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>

 	<animation>
		<type>noshadow</type>
		<object-name>BDME</object-name>
	</animation>

	<!--<animation include="Instrument-Light4.xml">
		<object-name>BDME</object-name>
	</animation>-->






	<animation>
		<type>rotate</type>
		<object-name>Interrupteur</object-name>
		<property>/instrumentation/distance-switch</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>



</PropertyList>
