	<animation>
		<type>rotate</type>
                <object-name>Input-Nav-Master</object-name>
		<property>/instrumentation/input/input-select</property>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>2</ind>
				<dep>-60</dep>
			</entry>
                        <center>
                            <x-m>-0.0303007</x-m>
                            <y-m>0.0354288</y-m>
                            <z-m>0</z-m>
		        </center>
			<axis>
				<x>0</x>
				<y>0</y>
				<z>1</z>
		          </axis>
	</animation>
        
        <animation>
            <type>pick</type>
            <object-name>Input-Nav-Master</object-name>
            <action>
                <button>0</button>
                <binding>
                    <command>property-adjust</command>
                    <property>/instrumentation/input/input-select</property>
                    <step>-30</step>
                    <min>0</min>
                    <max>60</max>
                </binding>
            </action>
        </animation>
        
        <animation>
            <type>pick</type>
            <object-name>Input-Nav-Master</object-name>
            <action>
                <button>1</button>
                <binding>
                    <command>property-adjust</command>
                    <property>/instrumentation/input/input-select</property>
                    <step>30</step>
                    <min>0</min>
                    <max>60</max>
                </binding>
            </action>
        </animation>


