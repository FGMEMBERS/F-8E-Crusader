        <animation>
            <type>pick</type>
            <object-name>Eclairage-InstrVol</object-name>
            <action>
                <button>0</button>
                <binding>
                    <command>property-adjust</command>
                    <property>/sim/model/f8e/instrument-lights</property>
                    <step>0.05</step>
                    <min>0</min>
                    <max>1</max>
                    <wrap>1</wrap>
                </binding>
            </action>
        </animation>

        <animation>
            <type>pick</type>
            <object-name>Eclairage-InstrVol</object-name>
            <action>
                <button>1</button>
                <binding>
                    <command>property-adjust</command>
                    <property>/sim/model/f8e/instrument-lights</property>
                    <step>-0.05</step>
                    <min>0</min>
                    <max>1</max>
                    <wrap>1</wrap>
                </binding>
            </action>
        </animation>


	<animation>
		<type>rotate</type>
		<object-name>Eclairage-InstrVol</object-name>
		<property>/sim/model/f8e/instrument-lights</property>
		<factor>-280</factor>
		<center>
			<x-m>0.00881384</x-m>
			<y-m>-0.0284904</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>

