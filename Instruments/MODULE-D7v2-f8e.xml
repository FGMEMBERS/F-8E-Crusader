 	</animation>
        
        <animation>
            <type>pick</type>
            <object-name>StoreAction</object-name>
            <action>
                <button>0</button>
                <binding>
                    <command>property-assign</command>
                    <property>sim/model/f8e/navaid/input/store</property>
                    <value>1</value>
                </binding>
                <mod-up>
                    <binding>
                        <command>property-assign</command>
                        <property>sim/model/f8e/navaid/input/store</property>
                        <value>0</value>
                    </binding>
                </mod-up>
            </action>
        </animation>
        
