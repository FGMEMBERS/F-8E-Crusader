    <!-- *******************************************************************************************************************-->
	
        
        <animation>
            <type>pick</type>
            <object-name>Bt-Val-nnn-XX</object-name>
            <action>
                <button>0</button>
                <repeatable type="bool">true</repeatable>
                <binding>
                    <command>property-adjust</command>
                    <property>sim/model/f8e/navaid/input/val-nnn-XX</property>
                    <step>5</step>
                    <min>0</min>
                    <max>100</max>
                    <resolution>1</resolution>
                    <wrap>1</wrap>
                </binding>
            </action>
            <action>
                <button>1</button>
                <repeatable type="bool">true</repeatable>
                <binding>
                    <command>property-adjust</command>
                    <property>sim/model/f8e/navaid/input/val-nnn-XX</property>
                    <step>-5</step>
                    <min>0</min>
                    <max>100</max>
                    <resolution>1</resolution>
                    <wrap>1</wrap>
                </binding>
            </action>
        </animation>
        
        <animation>
            <type>textranslate</type>
            <object-name>Freq-nnn-nX</object-name>
            <property>sim/model/f8e/navaid/input/val-nnn-XX</property>
            <factor>0.1</factor>
            <step>1</step>
            <scroll>1</scroll>
            <axis>
                <x>0</x>
                <y>-1</y>
                <z>0</z>
            </axis>
        </animation>
        
        <animation>
            <type>textranslate</type>
            <object-name>Freq-nnn-Xn</object-name>
