        <animation>
            <type>pick</type>
            <object-name>Inter-ModRW</object-name>
            <action>
                <repeatable type="bool">false</repeatable>
                <button>0</button>
                <binding>
                    <command>property-toggle</command>
                    <property>sim/model/f8e/navaid/input/modrw</property>
                </binding>
            </action>
        </animation>
        
	<animation>
		<condition>
			<equals>
				<property>sim/model/f8e/navaid/input/modrw</property>
				<value>1</value>
			</equals>
		</condition>
		<type>select</type>
		<object-name>base-write</object-name>
 	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Inter-ModRW</object-name>
		<property>sim/model/f8e/navaid/input/modrw</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>-30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0.0013260</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
 	</animation>
        
       
        
