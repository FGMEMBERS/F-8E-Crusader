	<animation>
		<type>select</type>
		<object-name>TemoinA-LeDroop</object-name>
		<condition>
			<and>
				<greater-than>
					<property>/surface-positions/leflap-pos-norm</property>
					<value>0</value>
				</greater-than>
				<not-equals>
					<property>/surface-positions/wings-pos-norm</property>
					<value>1</value>
				</not-equals>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>TemoinA-DuctByP</object-name>
		<condition>
				<equals>
					<property></property>
					<value>1</value>
				</equals>
		</condition>
	</animation>


