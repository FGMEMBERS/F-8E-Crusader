<PropertyList>

 <name>Panel Hotspots</name>
 <background>Aircraft/F-8E-Crusader/Panels/Textures/transparent-bg.rgb</background>
 <w>512</w>
 <h>384</h>
 <y-offset>0</y-offset>


 <instruments>
	<instrument>
		<name>Control</name>
		<x>256</x>
		<y>192</y>
		<w>512</w>
		<h>512</h>
		<w-base>256</w-base>
		<h-base>1024</h-base>
		<layers>
			<layer>
				<name>background</name>
				<texture>
					<path>Aircraft/F-8E-Crusader/Panels/Textures/transparent-bg.rgb</path>
					<!-- <path>Aircraft/F-8E-Crusader/Panels/Textures/hotspotgrid.rgb</path> -->
				<x1>0.0</x1>
				<y1>0.0</y1>
				<x2>1.0</x2>
				<y2>1.0</y2>
				</texture>
			</layer>
		</layers>

		<actions>



<!--************************************************************************************-->
<!--*********************************************Sasie des Frequences Nav**************************************-->
	<action>
		<name></name>
		<button>0</button>
		<x>50</x>
		<y>250</y>
		<w>100</w>
		<h>140</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/val-nnn-XX</property>
			<step>1</step>
			<min>0</min>
			<max>9</max>
			<resolution>1</resolution>
			<wrap>true</wrap>
		</binding>
	</action>

	<action>
		<name></name>
		<button>1</button>
		<x>50</x>
		<y>250</y>
		<w>100</w>
		<h>140</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/val-nnn-XX</property>
			<step>10</step>
			<min>0</min>
			<max>99</max>
			<resolution>1</resolution>
			<wrap>true</wrap>
		</binding>
	</action>

	<action>
		<name></name>
		<button>0</button>
		<x>50</x>
		<y>0</y>
		<w>100</w>
		<h>140</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/val-nnX-nn</property>
			<step>1</step>
			<min>0</min>
			<max>10</max>
			<resolution>1</resolution>
			<wrap>true</wrap>
		</binding>
	</action>

	<action>
		<name></name>
		<button>0</button>
		<x>50</x>
		<y>-240</y>
		<w>100</w>
		<h>140</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/val-XXn-nn</property>
			<step>1</step>
			<min>0</min>
			<max>9</max>
			<resolution>1</resolution>
			<wrap>true</wrap>
		</binding>
	</action>

	<action>
		<name></name>
		<button>1</button>
		<x>50</x>
		<y>-240</y>
		<w>100</w>
		<h>140</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/val-XXn-nn</property>
			<step>10</step>
			<min>0</min>
			<max>99</max>
			<resolution>1</resolution>
			<wrap>true</wrap>
		</binding>
	</action>

	<action>
		<name></name>
		<button>0</button>
		<x>-60</x>
		<y>-220</y>
		<w>80</w>
		<h>140</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/chan-XX</property>
			<step>1</step>
			<min>1</min>
			<max>21</max>
			<resolution>1</resolution>
			<wrap>true</wrap>
		</binding>
	</action>

	<action>
		<name></name>
		<button>1</button>
		<x>-60</x>
		<y>-220</y>
		<w>80</w>
		<h>140</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/chan-XX</property>
			<step>-1</step>
			<min>1</min>
			<max>21</max>
			<resolution>1</resolution>
			<wrap>true</wrap>
		</binding>
	</action>

	<action>
		<name></name>
		<button>0</button>
		<x>-30</x>
		<y>160</y>
		<w>40</w>
		<h>80</h>
		<binding>
			<command>property-assign</command>
			<property>/preset/input/store</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>/preset/input/store</property>
				<value>0</value>
			</binding>
		</mod-up>
	</action>

	<action>
		<name></name>
		<button>0</button>
		<x>0</x>
		<y>260</y>
		<w>40</w>
		<h>90</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/freqtype</property>
			<step>1</step>
			<min>0</min>
			<max>3</max>
			<resolution>1</resolution>
			<!--<wrap>true</wrap>-->
		</binding>
	</action>

	<action>
		<name></name>
		<button>1</button>
		<x>0</x>
		<y>260</y>
		<w>40</w>
		<h>90</h>
		<binding>
			<command>property-adjust</command>
			<property>/preset/input/freqtype</property>
			<step>-1</step>
			<min>0</min>
			<max>3</max>
			<resolution>1</resolution>
			<!--<wrap>true</wrap>-->
		</binding>
	</action>


	<action>
		<name></name>
		<button>0</button>
		<x>20</x>
		<y>-340</y>
		<w>40</w>
		<h>90</h>
		<binding>
			<command>property-toggle</command>
			<property>/preset/input/selector</property>
		</binding>
	</action>


	<action>
		<name></name>
		<button>0</button>
		<x>-30</x>
		<y>0</y>
		<w>60</w>
		<h>90</h>
		<binding>
			<command>property-toggle</command>
			<property>/preset/input/modrw</property>
		</binding>
	</action>






