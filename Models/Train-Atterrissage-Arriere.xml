	<!--            Spin  the weel              Fonction rotation Roue                -->

	<animation>
		<condition>
			<and>
				<equals>
					<property>controls/gear/gear-down</property>
					<value type="double">1.0</value>
				</equals>
				<less-than>
					<property>position/altitude-agl-ft</property>
					<value>5.5</value>
				</less-than>
			</and>
		</condition>
		<type>spin</type>
		<object-name>GTrain-Roue</object-name>
		<property>velocities/uBody-fps</property>
		<factor>10.00</factor>
		<axis>
			<x1-m>2.09447</x1-m>
			<y1-m>0.0622693</y1-m>
			<z1-m>-1.45047</z1-m>
			<x2-m>2.09447</x2-m>
			<y2-m>-2.93595</y2-m>
			<z2-m>-1.29333</z2-m>
		</axis>
	</animation>

