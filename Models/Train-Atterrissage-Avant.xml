	<!--            Spin  the Weel              Fonction rotation roue                -->

	<animation>
		<condition>
			<and>
				<equals>
					<property>controls/gear/gear-down</property>
					<value type="double">1.0</value>
				</equals>
                                <equals>
                                    <property>fdm/jsbsim/gear/unit/WOW</property>
                                    <value>1</value>
				</equals>
			</and>
		</condition>
		<type>spin</type>
		<object-name>AVTrain-Roue</object-name>
                <property>/fdm/jsbsim/gear/unit/wheel-speed-fps</property>
