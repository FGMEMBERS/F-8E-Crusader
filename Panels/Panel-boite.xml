<PropertyList>

	<path>Models/Panel-boite.ac</path>
	<texture-path>Models/Textures</texture-path>

	<animation>
		<type>noshadow</type>
		<object-name>panel-Boite</object-name>
	</animation>

	<animation include="../Instruments/Instrument-Light6.xml">
		<object-name>Panel-DBoite-ChkL</object-name>
