		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>-30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0.39262</y-m>
